{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "simple-server.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/simple-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
